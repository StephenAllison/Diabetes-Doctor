<h1>Athlete Database</h1>


<form action="/atheleteListPage" method="POST" id="form-container">


  <label for="league">League</label>
  <input id="league" type="text" name="league" placeholder="League">

 <br><br>
  <label for="name">Name</label>
  <input id="name" type="text" name="name" placeholder="Name">


 <br><br>
  <label for="position">Position</label>
  <input id="position" type="text" name="position" placeholder="Position">

 <br><br>
  <label for="university">University</label>
  <input id="university" type="text" name="university" placeholder="University">


  <br><br>
  <label for="points">Points</label>
  <input id="points" type="points" name="points" placeholder="Points">

    <br><br>
  <label for="rebounds">Rebounds</label>
  <input id="rebounds" type="rebounds" name="rebounds" placeholder="Rebounds">


  <br><br>
  <label for="assists">Assists</label>
  <input id="assists" type="assists" name="assists" placeholder="Assists">


  <br><br>
  <label for="turnovers">Turnovers</label>
  <input id="turnovers" type="turnovers" name="turnovers" placeholder="Turnovers">


  <br><br>
  <label for="blocks">Blocks</label>
  <input id="blocks" type="blocks" name="blocks" placeholder="Blocks">


  <br><br>
  <label for="steals">Steals</label>
  <input id="steals" type="steals" name="steals" placeholder="Steals">


  <br><br>
  <label for="fieldGoalPercentage:">fieldGoalPercentage:</label>
  <input id="fieldGoalPercentage:" type="fieldGoalPercentage:" name="fieldGoalPercentage:" placeholder="fieldGoalPercentage:">


  <br><br>
  <label for="threePointPercentage:">threePointPercentage:</label>
  <input id="threePointPercentage:" type="threePointPercentage:" name="threePointPercentage:" placeholder="threePointPercentage:">

  <br><br>
  <label for="freeThrowPercentage">freeThrowPercentage</label>
  <input id="freeThrowPercentage" type="freeThrowPercentage" name="freeThrowPercentage" placeholder="freeThrowPercentage">



  {{#if message}}
    <div class="error-message">{{ message }}</div>
  {{/if}}
  <br><br>

  <button>Create Athlete</button>

  <p class="account-message">
    Do you already have an account?
    <a href="/login">Login</a>
  </p>
</form>